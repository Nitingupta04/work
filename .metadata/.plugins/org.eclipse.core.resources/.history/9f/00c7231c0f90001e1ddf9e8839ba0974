package com.jtc.exceptionhandling1;

class Thread1 extends Thread {

}

public class Test {

	public static void main(String[] args) {
				
		Thread t1 = new Thread();
		t1.setName("Nitin");
		System.out.println("Thread t1 = "+t1);
		System.out.println("Current Thread :- "+Thread.currentThread().getName());
		System.out.println("Current Thread Group :- "+Thread.currentThread().getThreadGroup().getName());
		System.out.println("Default Priority " +t1.getPriority());
		t1.setPriority(2);
		System.out.println("Changed Priority " +t1.getPriority());		
		
		
		System.out.println("============\n");
		Thread1 t2 = new Thread1();
		t2.setName("My-First-User-Define-Thread");
		t2.setPriority(7);
		System.out.println(t2);
	
}

}


